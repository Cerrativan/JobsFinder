<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/myAdsDialog_descriptionTextView"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="12dp"
        android:text="Description"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/myAdsDescription_TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myAdsDialog_salaryTextView" />

    <Button
        android:id="@+id/myAdsDialog_deleteButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="228dp"
        android:backgroundTint="#D31E1E"
        android:text="Delete"
        android:textColor="#000000"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/myAdsDescription_TextView"
        android:layout_width="279dp"
        android:layout_height="194dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/myAdsDialog_descriptionTextView"
        app:layout_constraintTop_toBottomOf="@+id/myAdsSalary_textView" />

    <TextView
        android:id="@+id/myAdsDialog_salaryTextView"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="12dp"
        android:text="Salary"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/myAdsSalary_textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myAdsDialog_cityTextView" />

    <TextView
        android:id="@+id/myAdsDialog_cityTextView"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="12dp"
        android:text="City"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/myAdsCity_textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myAdsDialog_titleTextView" />

    <TextView
        android:id="@+id/myAdsDialog_titleTextView"
        android:layout_width="88dp"
        android:layout_height="25dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
        android:text="Title"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/myAdsTitle_textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/myAdsTitle_textView"
        android:layout_width="279dp"
        android:layout_height="25dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/myAdsDialog_titleTextView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/myAdsCity_textView"
        android:layout_width="279dp"
        android:layout_height="25dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="16dp"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/myAdsDialog_cityTextView"
        app:layout_constraintTop_toBottomOf="@+id/myAdsTitle_textView" />

    <TextView
        android:id="@+id/myAdsSalary_textView"
        android:layout_width="279dp"
        android:layout_height="25dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="16dp"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/myAdsDialog_salaryTextView"
        app:layout_constraintTop_toBottomOf="@+id/myAdsCity_textView" />

</androidx.constraintlayout.widget.ConstraintLayout>